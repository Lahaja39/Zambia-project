DirectoryIndex index.php index.html index.htm index2.html

ErrorDocument 400 http://connect-academy.com/pages/examples/404.php
ErrorDocument 401 http://connect-academy.com/pages/examples/404.php
ErrorDocument 403 http://connect-academy.com/pages/examples/404.php
ErrorDocument 404 http://connect-academy.com/pages/examples/404.php
ErrorDocument 500 http://connect-academy.com/pages/examples/500.php
ErrorDocument 503 http://connect-academy.com/pages/examples/404.php

SetEnv TZ Africa/Nairobi

# Disable Directory Browsing
Options All -Indexes
